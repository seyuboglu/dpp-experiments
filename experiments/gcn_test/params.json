{
    "n_processes": 1, 
    "device": "/device:GPU:0",
    "verbose": true,    
    "saliency_map": false,

    "ppi_network": "data/networks/bio-pathways-network.txt",
    "diseases_path": "data/associations/bio-pathways-associations.csv",
    "disease_subset": ["C0043459"], 
    
    "n_folds": 10,

    "method": "gcn",
    "model": "gcn", 
    "matrix": "adjacency", 

    "architecture": [["gcl", 128], ["fcl", 16]],

    "learning_rate": 0.01,
    "dropout": 0.5,
    "weight_decay": 5e-4,

    "epochs": 10,
    "early_stopping": 100, 
    "shuffle_negatives": true
}
